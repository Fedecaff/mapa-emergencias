{
  "version": 2,
  "builds": [
    {
      "src": "src/configuracion/servidor.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/configuracion/servidor.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/configuracion/servidor.js"
    }
  ]
}
